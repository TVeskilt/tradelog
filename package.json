{
  "name": "tradelog",
  "version": "1.0.0",
  "private": true,
  "description": "Custom options trading portfolio manager",
  "scripts": {
    "dev": "pnpm --parallel --filter \"@tradelog/web\" --filter \"@tradelog/api\" dev",
    "build": "pnpm --recursive build",
    "lint": "pnpm --recursive lint",
    "format": "prettier --write \"**/*.{ts,tsx,js,jsx,json,md,yaml,yml}\"",
    "type-check": "pnpm --recursive type-check"
  },
  "devDependencies": {
    "@typescript-eslint/eslint-plugin": "^7.0.0",
    "@typescript-eslint/parser": "^7.0.0",
    "eslint": "^8.56.0",
    "prettier": "^3.2.0",
    "typescript": "^5.3.3"
  },
  "engines": {
    "node": ">=22.0.0",
    "pnpm": ">=9.0.0"
  }
}
